{
    "name": "StefanieD/statemachine-bundle",
    "description": "Use statemachines for doctrine entities.",
    "keywords": ["symfony", "doctrine2", "extension", "statemachine"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefanie Drost",
            "email": "contact@stefaniedrost.com",
            "homepage": "http://stefaniedrost.com"
        }
    ],
    "require": {
        "php": "~5.4|~7.0",
        "symfony/framework-bundle": "~2.3|~3.0",
        "doctrine/common": "~2.2",
        "doctrine/doctrine-bundle": "~1.6",
        "yohang/finite": "~1.1",
        "alom/graphviz": "^1.1"
    },
    "require-dev": {
        "doctrine/orm": "~2.5",
        "symfony/console": "~2.3|~3.0",
        "symfony/twig-bundle": "~2.3|~3.0",
        "phpspec/phpspec": "~2.0",
        "jms/serializer-bundle": "~0.13"
    },
    "suggest": {
        "alom/graphviz": "To generate Graphviz views of state machines",
        "symfony/console": "To execute the command to generate Graphviz views of state machines"
    },
    "autoload": {
        "psr-0": { "SDrost\\StateMachineBundle": "" },
        "psr-4": { "SDrost\\StateMachineBundle\\": "" }
    },
    "target-dir": "SDrost/StateMachineBundle",
    "minimum-stability": "dev"
}
